from datagenerate import generate_data
import numpy as np
from est2d import local_p_2d

beta_0 = np.array([1/3, -2/3, 2/3])
theta_0 = np.array([2,1])

data = generate_data(100,5)
t = np.concatenate(data['t'])
x = np.concatenate(data['x'])
u = np.dot(x, beta_0)
print(u)

to_est = np.column_stack((t,u))
est = local_p_2d(data, bandwidth1= 0.2, bandwidth2= 0.1, xy0=to_est)
print(est)